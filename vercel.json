{
  "headers": [
    {
      "source": "/(.*\.(png|jpg|jpeg|gif|webp))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}
